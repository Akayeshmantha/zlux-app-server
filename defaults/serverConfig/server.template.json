{
  "node": {
    "allowInvalidTLSProxy": $ZWED_NODE_ALLOW_INVALID_TLS_PROXY,
    "https": {
      "ipAddresses": $ZWED_NODE_HTTPS_IPADDRESSES
      ,"port": $ZWED_NODE_HTTPS_PORT
      ,"keys": $ZWED_NODE_HTTPS_KEYS
      ,"certificates": $ZWED_NODE_HTTPS_CERTIFICATES
      ,"certificateAuthorities": $ZWED_NODE_HTTPS_CERTIFICATE_AUTHORITIES
    },
    "mediationLayer": {
      "enabled": $ZWED_MEDIATIONLAYER_ENABLED,
      "server": {
        "hostname": "$ZWED_MEDIATIONLAYER_SERVER_HOSTNAME"
        ,"port": $ZWED_MEDIATIONLAYER_SERVER_DISCOVERY_PORT
        ,"gatewayPort": $ZWED_MEDIATIONLAYER_SERVER_GATEWAY_PORT
        ,"isHttps": $ZWED_MEDIATIONLAYER_SERVER_ISHTTPS
      }
    },
    "agent": {
      "http": {
        "ipAddresses": $ZWED_AGENT_HTTP_IPADDRESSES
        ,"port": $ZWED_AGENT_HTTP_PORT
        ,"attls": $ZWED_AGENT_HTTP_ATTLS
      }
    }
    ,"childProcesses": $ZWED_CHILDPROCESSES
  }

  ,"siteDir": "$ZWED_DIR_SITE"
  ,"instanceDir": "$ZWED_DIR_INSTANCE"
  ,"groupsDir": "$ZWED_DIR_GROUP"
  ,"usersDir": "$ZWED_DIR_USERS"
  ,"pluginsDir":"$ZWED_DIR_PLUGINS"
  ,"logLanguage": "$ZWED_LOG_LANGUAGE"
  
  ,"dataserviceAuthentication": {
    "defaultAuthentication": "$ZWED_SERVICE_AUTH_DEFAULT"
    ,"rbac": $ZWED_SERVICE_AUTH_USE_RBAC,
    ,"implementationDefaults": $ZWED_SERVICE_AUTH_IMPLEMENTATION_DEFAULTS
  }
}
